quit() {
  # pkill zoom
  pkill -9 firefox
}

main() {
  if [[ -v "links[$1]" ]] ; then
    quit
    echo ${links[$1]} | xclip
    firefox --headless -url ${links[$1]}
  else
    quit
  fi 
}

main $@

